<body ng-app="myApp" ng-controller="DashboardController">

    <div class="navbar" ng-init="Login()">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="#/Index"><span><b>Bornomala ERP</b></span></a>
                <!-- start: Header Menu -->
                @Html.Partial("_LoginPartial")
                <!--End of Header Men-->
            </div>
        </div>
    </div>

    <div class="container-fluid-full">
        <div class="row-fluid">
            <!-- start: Main Menu -->
            <div id="sidebar-left" ng-hide="menu" ng-controller="DashboardController" class="span2">
                <div id="slim-scrollbar">
                    @Html.Partial("_Menu")
                </div>
            </div>
            <!-- end: Main Menu -->

            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>
                        You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.
                    </p>
                </div>
            </noscript>
            <!-- start: Jquery Elevator top-->
            <div id="top" class="elevator"></div>

            <!-- start: Content -->
            <div id="content" class="span10">
                <ul class="breadcrumb" ng-hide="pagename">
                    <li>
                        <i class="icon-home"></i>
                        <a href="#/Index">Home</a>
                        <i class="icon-angle-right"></i>
                    </li>
                    <li><a href="#">Dashboard</a></li>
                </ul>
                <div class="row-fluid">
                    <ng-view>
                        @RenderBody()
                    </ng-view>
                </div>
            </div>

            <div ng-hide="pagename" id="content" class="span10" style=" padding-bottom: 0px;">
                <footer>
                    <p style="text-align:right">
                        <span style="text-align:right !important;float:right">&copy; 2013 <a href="http://www.bornomalasystems.com" alt="Bornomala_Systems_Dashboard" style="color:#ffffff">Bornomala Systems Ltd.</a></span>
                    </p>
                </footer>
            </div>
        </div>
    </div>

    <div class="modal hide fade" id="myModal">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">×</button>
            <h3>Settings</h3>
        </div>
        <div class="modal-body">
            <p>Here settings can be configured...</p>
        </div>
        <div class="modal-footer">
            <a href="#" class="btn" data-dismiss="modal">Close</a>
            <a href="#" class="btn btn-primary">Save changes</a>
        </div>
    </div>

    <div class="clearfix"></div>

    <!--Jquery Elevatory Bottom-->
