    @*Enable Left Sidebar Scrolling - Using jquery.slimscroll.js*@
    <script>
        (function () {
            $(document).ready(function () {
                $('#slim-scrollbar').slimScroll({
                    height: '100%',
                    alwaysVisible: false,
                    color: '#FFA500',
                    size: '6px',
                    wheelStep: 10
                });
                /* Bottom to top // Elevator */
                $.elevator({
                    glass: true,
                    item_top: $('#content')
                });
            });
        })();
    </script>

